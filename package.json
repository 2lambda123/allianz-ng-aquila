{
    "name": "ng-aquila",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "ng-high-memory": "cross-env NODE_OPTIONS=--max-old-space-size=7680 ng",
        "start": "npm run build:package && npm run library:generate:docs && ng serve opensource-documentation",
        "build": "ng build",
        "build:all": "build:lib && build:docs",
        "build:lib": "npm run ng-high-memory -- build ng-aquila && npm run build:package",
        "build:docs": "npm run ng-high-memory -- build opensource-documentation",
        "build:package": "node ./scripts/build-package.js",
        "format": "prettier . --check",
        "format:fix": "prettier . --write",
        "lint": "ng lint && eslint projects/ng-aquila/src/schematics/**/*.ts && eslint projects/ngx-docs-cli/**/*.ts",
        "lint:fix": "ng lint --fix && eslint --fix projects/ng-aquila/src/schematics/**/*.ts && eslint --fix projects/ngx-docs-cli/**/*.ts",
        "postinstall": "node projects/ng-aquila/src/phone-input/extract-calling-codes.js",
        "prepare": "husky install &",
        "release": "standard-version -n",
        "test": "ng test",
        "test:lib": "npm run build:package && npm run test -- ng-aquila",
        "test:schematics": "npm run build:package && npm run test:schematics:jasmine",
        "test:schematics:jasmine": "jasmine --config=projects/ng-aquila/src/schematics/test/jasmine.json",
        "browserslist": "browserslist --coverage && browserslist",
        "browserslist:update": "browserslist --update-db",
        "library:generate:docs": "npm run cli -- generate projects/ng-aquila",
        "cli": "ts-node --project projects/ngx-docs-cli/tsconfig.json ./projects/ngx-docs-cli/src/main"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://www.github.com/aposin/ng-aquila.git"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.1.1",
        "@angular/cdk": "^13.1.1",
        "@angular/common": "^13.1.1",
        "@angular/compiler": "^13.1.1",
        "@angular/core": "^13.1.1",
        "@angular/forms": "^13.1.1",
        "@angular/platform-browser": "^13.1.1",
        "@angular/platform-browser-dynamic": "^13.1.1",
        "@angular/router": "^13.1.1",
        "@fortawesome/fontawesome-free": "^5.15.4",
        "chalk": "^4.1.2",
        "dayjs": "^1.10.7",
        "decimal.js": "^10.3.1",
        "highlight.js": "^10.7.3",
        "i18n-iso-countries": "^6.8.0",
        "iban": "^0.0.14",
        "libphonenumber-js": "^1.9.44",
        "moment": "^2.29.1",
        "ngx-color-picker": "^11.0.0",
        "parse-color": "^1.0.0",
        "rxjs": "^6.6.7",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.1.2",
        "@angular-devkit/schematics-cli": "^13.1.2",
        "@angular-eslint/builder": "13.0.1",
        "@angular-eslint/eslint-plugin": "13.0.1",
        "@angular-eslint/eslint-plugin-template": "13.0.1",
        "@angular-eslint/schematics": "13.0.1",
        "@angular-eslint/template-parser": "13.0.1",
        "@angular/cli": "^13.1.2",
        "@angular/compiler-cli": "^13.1.1",
        "@angular/language-service": "^13.1.1",
        "@angular/localize": "^13.1.1",
        "@commitlint/cli": "16.0.1",
        "@commitlint/config-angular": "16.0.0",
        "@types/fs-extra": "9.0.13",
        "@types/iban": "0.0.32",
        "@types/jasmine": "^3.10.2",
        "@types/node": "^17.0.5",
        "@types/parse-color": "^1.0.1",
        "@types/shelljs": "^0.8.10",
        "@typescript-eslint/eslint-plugin": "5.3.0",
        "@typescript-eslint/parser": "5.3.0",
        "axe-core": "4.3.5",
        "core-js": "^3.20.1",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "dgeni": "~0.4.14",
        "dgeni-packages": "~0.28.4",
        "eslint": "8.2.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-deprecation": "1.3.2",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-jasmine": "4.1.3",
        "eslint-plugin-regexp": "1.5.1",
        "eslint-plugin-rxjs": "5.0.1",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "front-matter": "^4.0.2",
        "fs-extra": "^9.1.0",
        "husky": "^7.0.4",
        "jasmine": "^3.10.0",
        "jasmine-core": "^3.10.1",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.3.9",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "^2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "karma-junit-reporter": "^2.0.1",
        "karma-mocha-reporter": "^2.2.5",
        "karma-viewport": "^1.0.8",
        "lint-staged": "^12.1.4",
        "markdown-it": "^12.3.0",
        "markdown-it-anchor": "^6.0.1",
        "ng-packagr": "^13.1.2",
        "prettier": "2.5.1",
        "pretty-quick": "^3.1.3",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.4",
        "standard-version": "^9.3.2",
        "ts-node": "^10.4.0",
        "typescript": "4.4.4"
    }
}
