{
    "name": "ng-aquila",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "ng-high-memory": "cross-env NODE_OPTIONS=--max-old-space-size=7680 ng",
        "release": "standard-version -n",
        "start": "npm run build:package && npm run library:generate:docs && ng serve opensource-documentation --port=4200",
        "build": "ng build",
        "test": "ng test",
        "format": "pretty-quick --check",
        "lint:ts": "tslint -c projects/ng-aquila/tslint.json -p projects/ng-aquila/tsconfig.lib.json",
        "lint": "npm run lint:ts",
        "lint:commitmsg": "commitlint -e",
        "lint-staged": "lint-staged --relative",
        "build:all": "npm-run-all build:lib build:docs",
        "build:lib": "npm run ng-high-memory -- build ng-aquila --configuration production && npm run build:package",
        "build:docs": "npm run ng-high-memory -- build opensource-documentation --configuration production",
        "postinstall": "node projects/ng-aquila/src/phone-input/extract-calling-codes.js",
        "test:lib": "npm run build:package && npm run test -- ng-aquila",
        "test:schematics": "npm run build:package && npm run test:schematics:jasmine",
        "test:schematics:jasmine": "jasmine --config=projects/ng-aquila/src/schematics/test/jasmine.json",
        "build:package": "node ./scripts/build-package.js",
        "library:generate:docs": "npm run cli -- generate projects/ng-aquila",
        "cli": "ts-node --project projects/ngx-docs-cli/tsconfig.json ./projects/ngx-docs-cli/src/main"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://www.github.com/aposin/ng-aquila.git"
    },
    "husky": {
        "hooks": {
            "commit-msg": "npm run lint:commitmsg",
            "pre-commit": "npm run lint-staged && pretty-quick --staged"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.0.3",
        "@angular/cdk": "^13.0.3",
        "@angular/common": "^13.0.3",
        "@angular/compiler": "^13.0.3",
        "@angular/core": "^13.0.3",
        "@angular/forms": "^13.0.3",
        "@angular/platform-browser": "^13.0.3",
        "@angular/platform-browser-dynamic": "^13.0.3",
        "@angular/router": "^13.0.3",
        "@fortawesome/fontawesome-free": "^5.15.4",
        "chalk": "^4.1.2",
        "core-js": "^3.18.3",
        "dayjs": "^1.10.7",
        "decimal.js": "^10.3.1",
        "dgeni": "^0.4.14",
        "dgeni-packages": "^0.28.4",
        "front-matter": "^4.0.2",
        "fs-extra": "^9.1.0",
        "highlight.js": "^10.7.3",
        "i18n-iso-countries": "^6.8.0",
        "iban": "^0.0.14",
        "libphonenumber-js": "^1.9.38",
        "markdown-it": "^12.2.0",
        "markdown-it-anchor": "^6.0.1",
        "moment": "^2.29.1",
        "ngx-color-picker": "^11.0.0",
        "parse-color": "^1.0.0",
        "rxjs": "^6.6.7",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.0.4",
        "@angular-devkit/schematics-cli": "^13.0.4",
        "@angular/cli": "^13.0.4",
        "@angular/compiler-cli": "^13.0.3",
        "@angular/language-service": "^13.0.2",
        "@angular/localize": "^13.0.2",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-angular": "^11.0.0",
        "@types/fs-extra": "9.0.6",
        "@types/iban": "0.0.32",
        "@types/jasmine": "^3.10.0",
        "@types/node": "^12.20.33",
        "@types/parse-color": "^1.0.1",
        "@types/shelljs": "^0.8.9",
        "axe-core": "^4.3.3",
        "codelyzer": "^6.0.2",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "husky": "^4.3.8",
        "jasmine": "^3.10.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "^6.3.8",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "karma-junit-reporter": "^2.0.1",
        "karma-mocha-reporter": "^2.2.5",
        "karma-viewport": "^1.0.8",
        "lint-staged": "^10.5.4",
        "ng-packagr": "^13.0.8",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.2",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.4",
        "standard-version": "^9.3.2",
        "ts-node": "^9.1.1",
        "tslint": "~6.1.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-jasmine-rules": "^1.6.1",
        "typescript": "4.4.4"
    }
}
